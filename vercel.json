{
  "rewrites": [
    {
      "source": "/",
      "destination": "/app/index.html"
    },
    {
      "source": "/script.js",
      "destination": "/app/script.js"
    }
  ],
  "headers": [
    {
      "source": "/app/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    }
  ]
}
